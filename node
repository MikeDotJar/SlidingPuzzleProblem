class Node {
    
    int[] puzzle;
    Node up,down,left,right,parent;
    
    public Node(int[] puzzle){
        this.puzzle = puzzle;
        up = null;
        down = null;
        left = null;
        right = null;
        parent = null;
    }
    
}
